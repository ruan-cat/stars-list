{
	"name": "@ruan-cat-docs/stars-list",
	"private": true,
	"type": "module",
	"version": "0.0.0",
	"packageManager": "pnpm@10.16.1",
	"engines": {
		"node": ">=22.14.0"
	},
	"author": "ruan-cat",
	"description": "阮喵喵的github star列表，是一个基于工作流自动更新的文档项目。",
	"homepage": "https://ruan-cat.github.io/stars-list",
	"main": "index.js",
	"scripts": {
		"git:fetch": "git fetch -p",
		"git:dev-2-master": "git rebase dev master && git push --follow-tags",
		"git:master-2-dev": "git rebase master dev",
		"up-taze": "pnpm up @ruan-cat/taze-config -L && npx taze -r",
		"format": "prettier --experimental-cli --write '**/*.{js,jsx,ts,tsx,mts,json,css,scss,md,yml,yaml,html}'",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"docs:build-in-github-page": "vitepress build docs --base=/stars-list/",
		"build": "pnpm docs:build-in-github-page"
	},
	"devDependencies": {
		"@prettier/plugin-oxc": "^0.1.3",
		"@ruan-cat/commitlint-config": "^3.1.0",
		"@ruan-cat/taze-config": "^0.2.0",
		"@ruan-cat/vitepress-preset-config": "^2.3.0",
		"commitizen": "^4.3.1",
		"cz-git": "^1.12.0",
		"prettier": "^3.7.4",
		"prettier-plugin-lint-md": "^1.0.1",
		"taze": "^19.6.0",
		"vitepress": "^1.6.4",
		"vitepress-demo-plugin": "^1.4.7",
		"vue": "^3.5.21"
	},
	"keywords": [
		"ruan-cat",
		"github",
		"stars",
		"list"
	],
	"license": "MIT"
}